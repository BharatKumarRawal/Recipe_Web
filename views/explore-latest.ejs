<h1 class="pb-4">Latest Recipes</h1>
<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
  <% if (latestRecipe != "") { %> <%
  latestRecipe.forEach((element, index) => { %>
  <a
    href="/recipe/<%- element._id  %>"
    class="col text-center category__link"
  >
    <div class="category__img category__img--large shadow">
      <img src="/uploads/<%- element.image  %>" alt="" loading="lazy" />
    </div>
    <div class="pt-1"><%- element.name %></div>
  </a>
  <% }) %> <% } else { %>
  <p>No Items Found</p>
  <% } %>
</div>